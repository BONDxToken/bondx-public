(mod (EPOCH_SECONDS EPOCH_OFFSET_UTC)
  (defun epoch_of (timestamp)
    (div (max 0 (- timestamp EPOCH_OFFSET_UTC)) EPOCH_SECONDS)
  )
  (defun epoch_start_time (epoch_n)
    (+ EPOCH_OFFSET_UTC (* epoch_n EPOCH_SECONDS))
  )
  ;; floor(0.5% of x) = floor(x / 200)
  (defun half_percent_floor (x)
    (div x 200)
  )
  (defun min (a b) (if (< a b) a b))
  (defun max (a b) (if (< a b) b a))
  (list epoch_of epoch_start_time half_percent_floor min max)
)
